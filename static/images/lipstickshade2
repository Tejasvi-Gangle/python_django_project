		const elements = [...document.querySelectorAll(selector)];

		return elements.filter(
		  (element) =>
			element.childNodes[0]
			&& element.childNodes[0].nodeValue
			&& RegExp(text, "u").test(element.childNodes[0].nodeValue.trim())
		);
	}
